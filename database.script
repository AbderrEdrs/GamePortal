CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE JEU(IDJEU INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,DATE_CREATION_JEU TIMESTAMP,NOM_JEU VARCHAR(255),TYPE_JEU VARCHAR(255))
CREATE MEMORY TABLE USER(IDUSER INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,AVATAR VARCHAR(255),EMAIL VARCHAR(255),FIRSTNAME VARCHAR(255),LASTNAME VARCHAR(255),MODIFIED TIMESTAMP,PASSWORD VARCHAR(255),USERNAME VARCHAR(255))
ALTER TABLE JEU ALTER COLUMN IDJEU RESTART WITH 1
ALTER TABLE USER ALTER COLUMN IDUSER RESTART WITH 1
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
